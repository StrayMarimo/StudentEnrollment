<p style="color: green"><%= notice %></p>

<h1>Students</h1>

<div id="students">
  <table>
    <thead>
      <th>Student Name</th>
      <th>Year Level</th>
    </thead>
    <tbody>
      <% @students.each do |student| %>
      <tr>
        <td> <%= link_to student.name, student %></td>
        <td><%= student.year_level%></td>
      </tr>
       <% end %>
    </tbody>
  </table>
</div>
<br>
<%= link_to "New student", new_student_path %>
